@Article{BagschikFroemterMuellerEtAl2016a,
  author    = {Kai Bagschik and Robert Fr\"{o}mter and Leonard M\"{u}ller and Wojciech Roseker and Judith Bach and Philipp Staeck and Carsten Th\"{o}nni{\ss}en and Stefan Schleitzer and Magnus H{\aa}rdensson Berntsen and Christian Weier and Roman Adam and Jens Viefhaus and Claus Michael Schneider and Gerhard Gr\"{u}bel and Hans Peter Oepen},
  title     = {Spatial coherence determination from the Fourier analysis of a resonant soft X-ray magnetic speckle pattern},
  journal   = {Opt. Express},
  year      = {2016},
  volume    = {24},
  number    = {20},
  month     = {Oct},
  pages     = {23162--23176},
  doi       = {10.1364/OE.24.023162},
  url       = {http://www.opticsexpress.org/abstract.cfm?URI=oe-24-20-23162},
  abstract  = {We present a method to determine the two-dimensional spatial coherence of synchrotron radiation in the soft X-ray regime by analyzing the Fourier transform of the magnetic speckle pattern from a ferromagnetic film in a multidomain state. To corroborate the results, a Young\&\#x2019;s double-pinhole experiment has been performed. The transverse coherence lengths in vertical and horizontal direction of both approaches are in a good agreement. The method presented here is simple and gives a direct access to the coherence properties of synchrotron radiation without nanostructured test objects.},
  file      = {:BagschikFroemterMuellerEtAl2016a.pdf:PDF},
  groups    = {DP-experiment},
  keywords  = {Coherence; Speckle; Metals; Thin films; Scattering measurements; Synchrotron radiation; Coherence; Far field diffraction; Laser light; Physical vapor deposition; Ptychography; Speckle patterns},
  publisher = {OSA},
  timestamp = {2018-09-19},
}

