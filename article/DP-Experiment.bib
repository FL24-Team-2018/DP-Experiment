@Article{PedreiraSicsSorrentinoEtAl2016,
  author    = {P. Pedreira and I. Sics and A. Sorrentino and E. Pereiro and L. Aballe and M. Foerster and V. P{\'{e}}rez-Dieste and C. Escudero and J. Nicolas},
  title     = {Optical pseudomotors for soft x-ray beamlines},
  journal   = {Review of Scientific Instruments},
  year      = {2016},
  volume    = {87},
  number    = {5},
  month     = {5},
  pages     = {052002},
  doi       = {10.1063/1.4949339},
  url       = {http://dx.doi.org/10.1063/1.4949339},
  abstract  = {Optical elements of soft x-ray beamlines usually have motorized translations and rotations that allow for the fine alignment of the beamline. This is to steer the photon beam at some positions and to correct the focus on slits or on sample. Generally, each degree of freedom of a mirror induces a change of several parameters of the beam. Inversely, several motions are required to actuate on a single optical parameter, keeping the others unchanged. We define optical pseudomotors as combinations of physical motions of the optical elements of a beamline, which allow modifying one optical parameter without affecting the others. We describe a method to obtain analytic relationships between physical motions of mirrors and the corresponding variations of the beam parameters. This method has been implemented and tested at two beamlines at ALBA, where it is used to control the focus of the photon beam and its position independently.},
  file      = {PedreiraSicsSorrentinoEtAl2016.pdf:PedreiraSicsSorrentinoEtAl2016.pdf:PDF},
  publisher = {{AIP} Publishing},
  timestamp = {2016-07-13},
}

